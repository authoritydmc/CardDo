<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.authoritydmc.cardKart">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CardDo">
        <activity
            android:name="com.authoritydmc.cardKart.activities.UpdateActivity"
            android:theme="@style/Theme.CardDo.NoactionBar"
            android:exported="false" />
        <activity
            android:name="com.authoritydmc.cardKart.activities.Settings"
            android:exported="false" />
        <activity
            android:name="com.authoritydmc.cardKart.activities.Splash"
            android:exported="true"
            android:theme="@style/Theme.CardDo.NoactionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.authoritydmc.cardKart.activities.MainActivity"
            android:exported="false"></activity>
    </application>

</manifest>